{
  "name": "mure",
  "version": "0.1.1",
  "description": "An integration library for the mure ecosystem of apps",
  "main": "build/mure.umd.min.js",
  "module": "build/mure.es.js",
  "jsnext:main": "build/mure.es.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build_umd": "./node_modules/.bin/rollup -c rollup.umd.config.js && ./node_modules/.bin/uglifyjs ./build/mure.umd.js -c -m -o ./build/mure.umd.min.js",
    "build_es": "./node_modules/.bin/rollup -c rollup.es.config.js",
    "build": "npm run build_umd; npm run build_es"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mure-apps/mure-library.git"
  },
  "author": "Alex Bigelow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mure-apps/mure-library/issues"
  },
  "homepage": "https://github.com/mure-apps/mure-library#readme",
  "devDependencies": {
    "ansi-256-colors": "^1.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-json": "^2.3.0",
    "uglify-js": "^3.0.27"
  },
  "dependencies": {
    "pouchdb": "^6.3.4",
    "uki": "^0.1.0"
  }
}
